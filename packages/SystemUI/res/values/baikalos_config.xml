<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>

    <!-- The CPU temperature sensor path, defaults to empty -->
    <string name="config_cpuTempSensorPrefix" translatable="false"></string>
    <!-- The CPU temperature sensor path, defaults to empty -->
    <string name="config_cpuTempSensorSuffix" translatable="false"></string>
    <!-- The CPU temperature sensor path, defaults to empty -->
    <string name="config_cpuTempSensorNumber" translatable="false"></string>

    <!-- The CPU temperature sensor path, defaults to empty -->
    <string name="config_cpuTempSensor" translatable="false"></string>

    <!-- The CPU temperature divider, if needed -->
    <integer name="config_cpuTempDivider" translatable="false">1</integer>

    <!-- The SYS temperature divider, if needed -->
    <integer name="config_sysTempDivider" translatable="false">1</integer>

    <!-- The BAT temperature divider, if needed -->
    <integer name="config_batTempDivider" translatable="false">10</integer>

    <!-- The SYS temperature sensor path, defaults to empty -->
    <string name="config_sysTempSensor" translatable="false"></string>

    <!-- The BAT temperature sensor path, defaults to empty -->
    <string name="config_batTempSensor" translatable="false">/sys/class/power_supply/battery/temp</string>

    <!-- The GPU frequency sensor path, defaults to empty -->
    <string name="config_gpuFreqSensor" translatable="false">/sys/devices/soc/b00000.qcom,kgsl-3d0/kgsl/kgsl-3d0/clock_mhz</string>

    <!-- Asus stitch editor component name -->
    <string name="config_stitchEditor">com.asus.stitchimage/com.asus.stitchimage.editor.ScreenshotEditorActivity</string>

    <!-- FPSInfoService Refresh Rate node file path -->
    <string name="config_rrInfoSysNode" translatable="false"></string>

</resources>
